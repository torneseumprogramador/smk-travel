                @Html.DisplayNameFor(model => model.Taxa1)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Funcionario.Codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Itinerario.Codigo)
